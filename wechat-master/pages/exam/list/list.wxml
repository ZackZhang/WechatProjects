<view class='exam-container'>
  <!-- 目录模块 -->
  <view class='weui-grids'>
    <block wx:for="{{examList}}" wx:key="{{item}}">
      <view data-desc='{{item.label}}' data-id='{{item.id}}' class="weui-grid custom-style" hover-class="weui-grid_active" bindtap='toCatalog'>
        <image class="weui-grid__icon " src="{{ item.iconUrl }} " />
      </view>
    </block>
  </view>
</view>