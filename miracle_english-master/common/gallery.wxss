.gallery {
    display: flex;
    flex-wrap: wrap;
    position: relative;
}

.gallery-item {
    width: 190rpx;
    height: 190rpx;
    margin: 0 15rpx 15rpx 0;
    position: relative;
}

.gallery-item-cover {
    width: 100%;
    height: 100%;
}

.gallery-item-del {
  position: absolute;
  top: -13rpx;
  right: -13rpx;
  height: 26rpx;
}

.gallery-item-del image {
  display: block;
  width: 26rpx;
  height: 26rpx;
}