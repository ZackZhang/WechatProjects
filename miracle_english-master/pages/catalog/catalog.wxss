/* pages/catalog/catalog.wxss */
.page {
  background: linear-gradient(#195791 70%, #19ec8c);
}

.qjyy-ban image {
  width: 750rpx;
  height: 444rpx;
}

.catalog-scroll {
  height: 1060rpx;
  overflow: hidden;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
}

.catalog-item {
  border-radius: 10rpx;
  background: #f4624e;
  box-shadow: 0 5rpx 13rpx rgba(244, 98, 78, .47);
}

.catalog-item:nth-of-type(6n+2) {
  background: #f7af32;
  box-shadow: 0 5rpx 13rpx rgba(247, 175, 50, .47);
}

.catalog-item:nth-of-type(6n+3) {
  background: #12d66e;
  box-shadow: 0 5rpx 13rpx rgba(18, 214, 110, .47);
}

.catalog-item:nth-of-type(6n+4) {
  background: #02c8d4;
  box-shadow: 0 5rpx 13rpx rgba(2, 200, 212, .47);
}

.catalog-item:nth-of-type(6n+5) {
  background: #1b90da;
  box-shadow: 0 5rpx 13rpx rgba(2, 200, 212, .47);
}

.catalog-item:nth-of-type(6n+6) {
  background: #b374ee;
  box-shadow: 0 5rpx 13rpx rgba(179, 116, 238, .47);
}

.catalog-item:not(:first-child) {
  margin-top: 20rpx;
}

.catalog-item .item-img {
  height: 120rpx;
}

.catalog-item .item-img image {
  width: 193rpx;
  height: 120rpx;
  border-radius: 6rpx;
}

.search-input {
  display: inline-block;
  width: 200rpx;
  height: 40rpx;
  line-height: 38rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  border-radius: 6rpx;
  background: #c1ffde;
  border: 1rpx solid #2f6d9b;
}

.menu-list {
  padding: 0 40rpx;
}

.menu-item {
  padding: 10rpx 40rpx;
}

.menu-item .item-inner {
  padding: 20rpx 10rpx;
  background: #1b90da;
  border-radius: 10rpx;
  box-shadow: 0 0 19rpx rgba(0, 140, 105, .67);
}

.menu-item .item-icon {
  height: 70rpx;
}

.custom-btn {
  width: 230rpx;
  height: 100rpx;
  line-height: 100rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
}