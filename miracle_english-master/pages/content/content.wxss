/* pages/content/content.wxss */
.hb {
  background: linear-gradient(#0050a4, #017a9e);  
}

.hb-tle image {
  width: 750rpx;
  height: 222rpx;
}

.bd-tle {
  margin-top: -60rpx;
  color: #fbf50a;
}

.bd-txt view {
  text-indent: 2em;
}

.bd-audio {
  position: relative;
  border-radius: 15rpx;
  border: 4rpx solid #63b7ff;
}

.bd-audio:after {
  position: absolute;
  top: -15rpx;
  left: 50%;
  width: 30rpx;
  height: 30rpx;
  background: #fbf50a;
  border: 6rpx solid #63b7ff;
  border-radius: 50%;
  content: "";
  transform: translate(-50%, 0);
}

.bd-audio .audio-tle {
  color: #28548e;
}

.bd-audio .audio-txt {
  font-family: "Arial";
  letter-spacing: 1px;
}

.bd-audio .audio-txt view {
  text-indent: 2em;
}

.bd-audio .audio-txt .underline {
  border-bottom: 2rpx solid #68b9ff;
}

.bd-audio .audio-txt .highlight {
  color: #e46b00;
  font-weight: 700;
}

.bd-audio .audio-img image {
  width: 619rpx;
  height: 350rpx;
}

.bd-audio .audio-control {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -70%);
}

.bd-audio .audio-control {
  opacity: .8;
}

.bd-audio .drag {
  border-radius: 20rpx;
}

.bd-audio .audio-progress {
  position: relative;
}

.bd-audio .audio-original {
  position: absolute;
  top: -30rpx;
  right: 0;
  height: 42rpx;
  line-height: 36rpx;
  padding: 0 10rpx;
  background: #42a8ff;
  border-radius: 20rpx;
}

.hb-mod {
  position: relative;
  background: linear-gradient(#017a9e, #029d98);
}

.hb-mod--gdxx {
  background: linear-gradient(#029d98, #03ca92);
}

.hb-mod--xztfx {
  background: linear-gradient(#1283ca, #31cace);
}

.hb-mod:after {
  position: absolute;
  top: 260rpx;
  left: 0;
  right: 0;
  height: 90rpx;
  background: #7ebf52;
  content: "";
  transform: skewY(-5deg);
}

.hb-mod--no-bg:after {
  content: none;
}

.hb-mod .mod-tle {
  color: #fbf50a;
}

.hb-mod .mod-content {
  position: relative;
  z-index: 1;
  border-radius: 10rpx;
}

.circle-icon {
  display: inline-block;
  width: 62rpx;
  height: 62rpx;
  line-height: 62rpx;
  font-size: 42rpx;
  color: #fff;
  text-align: center;
  background: #42a8ff;
  border-radius: 50%;
}

.zaoju-item {
  padding: 40rpx;
}

.zaoju-item:not(:first-child) {
  border-top: 1rpx solid #f2f6fa;
}

.zaoju-item .item-input {
  height: 70rpx;
  line-height: 70rpx;
  padding: 0 20rpx;
  background: #f2f6fa;
  border-radius: 35rpx;
}

.zaoju-item .item-right {
  width: 44rpx;
  height: 44rpx;
  line-height: 44rpx;
  text-align: center;
  background: #fff;
  border-radius: 50%;
  box-shadow: 0 0 17rpx rgba(210, 210, 210, .32);
}

.sound-wave {
  position: relative;
  height: 84rpx;
  line-height: 84rpx;
  text-align: center;
  background: #e2f2ff;
  border-radius: 42rpx;
}

.sound {
  width: 176rpx;
  height: 40rpx;
  vertical-align: middle;
}

.wave-list {
  line-height: 64rpx;
}

.wave-item {
  width: 4rpx;
  height: 30rpx;
  margin: 0 4rpx;
  background: #0095e3;
  vertical-align: bottom;
}

.wave-item.active:nth-of-type(1) {
  animation: wave .6s linear infinite;
}

.wave-item.active:nth-of-type(2) {
  animation: wave 0.7s linear infinite;
}

.wave-item.active:nth-of-type(3) {
  animation: wave .8s linear infinite;
}

.wave-item.active:nth-of-type(4) {
  animation: wave 0.9s linear infinite;
}

.wave-item.active:nth-of-type(5) {
  animation: wave 0.5s linear infinite;
}

.wave-item.active:nth-of-type(6) {
  animation: wave .9s linear infinite;
}

.wave-item.active:nth-of-type(7) {
  animation: wave .8s linear infinite;
}

.wave-item.active:nth-of-type(8) {
  animation: wave .6s linear infinite;
}

.wave-item.active:nth-of-type(9) {
  animation: wave .7s linear infinite;
}

.wave-item.active:nth-of-type(10) {
  animation: wave .5s linear infinite;
}

@keyframes wave{
	0% { height: 15rpx }
	50% { height: 40rpx }
	100% { height: 18rpx }
}

.color-primary {
  color: #57b7f5 !important;
}

.answer-item {
  margin-top: 30rpx;
  color: #194484;
}

.answer-item.active {
  color: #e46b00;
}

.answer-item.active .item-id {
  color: #fbf552;
  background: #1283ca;
}

.answer-item .item-id {
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  background: #fbf552;
  font-size: 24rpx;
  color: #194484;
  text-transform: uppercase;
  text-align: center;
  border-radius: 50%;
}

/* 弹窗 */
.modal {
  background: none;
}

.modal-txt {
  padding: 0 50rpx;
  font-size: 20rpx;
  color: #00fff0;
}

.share-container {
  position: relative;
  width: 550rpx;
  margin: 0 auto;
  padding: 180rpx 0 85rpx;
  border-radius: 15rpx;
  background: linear-gradient(#066cc9, #00fef0);
}

.user-info {
  top: -45rpx;
}

.txt-thanks image {
  width: 390rpx;
  height: 111rpx;
}

.txt-zhuanfa {
  margin-top: 60rpx;
}

.txt-zhuanfa text {
  color: #f8de01;
}

.get-list {
  margin-left: 70rpx;
  margin-right: 70rpx;
  font-size: 18rpx;
  color: #0589d1;
  border: 2rpx solid #058bd2;
  border-radius: 30rpx;
}

.share-btn {
  position: relative;
  margin: 90rpx 0 0;
  display: inline-block;
}

.btn-share {
  height: 60rpx;
  line-height: 60rpx;
  overflow: visible;
  padding: 0 40rpx;
  font-size: 32rpx;
  color: #01e0e8;
  border-radius: 15rpx;
  background: #fff;
}

.share-btn:after {
  position: absolute;
  top: -58rpx;
  right: 0;
  width: 141rpx;
  height: 48rpx;
  background: url("data:image/png;base64,") no-repeat right center;
  background-size: 100%;
  content: "";
}

.modal-tips {
  width: 634rpx;
  height: 896rpx;
  padding: 130rpx 40rpx 0;
  box-sizing: border-box;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
}

.modal-tips .tips-tle {
  font-size: 40rpx;
  text-shadow: 3rpx 3rpx 3rpx rgba(0, 0, 0, .26);
}

.modal-tips .tips-tle text,
.modal-tips .tips-txt text {
  color: #f8de01;
}

.modal-tips .tips-txt {
  border-top: 2rpx dashed #f8de01;
}

.tips-btn {
  width: 634rpx;
  padding: 15rpx 0;
  font-size: 40rpx;
  color: #f8de01;
  text-shadow: 3rpx 3rpx 3rpx rgba(0, 0, 0, .75);
  border-radius: 50rpx;
  background: linear-gradient(#00fbef, #0670ca);
}

.tips-btn text {
  font-size: 50rpx;
}